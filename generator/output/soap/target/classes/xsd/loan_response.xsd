<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.example.org/loanapp/response"
           xmlns:tns="http://www.example.org/loanapp/response"
           elementFormDefault="qualified">
 
    <xs:element name="LoanApplicationResponse">
<xs:complexType>
<xs:sequence>
<xs:element name="loanID" type="xs:string"/>
<xs:element name="status" type="xs:string"/>
<xs:element name="message" type="xs:string"/>
<xs:element name="rejectionReason" type="xs:string" minOccurs="0"/>
<xs:element name="approvedAmount" type="xs:decimal" minOccurs="0"/>
</xs:sequence>
</xs:complexType>
</xs:element>
 
</xs:schema>